{
  "name": "react-tdd-boilerplate",
  "version": "1.0.0",
  "description": "This is a React & Redux boilerplate with Mocha, Enzyme and Chai already set. You can develop your app by using TDD convension.",
  "main": "index.js",
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "build-url": "^1.0.10",
    "css-loader": "^0.28.7",
    "path": "^0.12.7",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-tippy": "^1.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "style-loader": "^0.19.1",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
  },
  "devDependencies": {
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "ignore-styles": "^5.0.1",
    "jsdom": "^11.5.1",
    "mocha": "^4.0.1",
    "react-addons-test-utils": "^15.6.2",
    "redux-mock-store": "^1.4.0",
    "redux-test-utils": "^0.2.2",
    "sinon": "^4.1.3",
    "sinon-chai": "^2.14.0",
    "url-loader": "^0.6.2"
  },
  "scripts": {
    "build": "webpack",
    "dev:hot": "webpack-dev-server --hot --inline --progress --colors --watch",
    "test": "mocha --require babel-register --require ignore-styles test/helpers/config.js test/**/*.spec.js src/**/*.js",
    "test:tdd": "mocha --require babel-register --require ignore-styles --watch test/helpers/config.js test/**/*.spec.js src/**/*.js"
  },
  "engines": {
    "node": ">=8.9.3"
  },
  "author": "",
  "license": "ISC"
}
